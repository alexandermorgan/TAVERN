printf '\n\nHarmonic A\n'
extractx -s 2 *a.krn  | ridx -dCMI | sed 's/^[0-9][0-9]*\.*//g' | grep -v '^!!!' | sort | uniq 

printf '\n\nHarmonic B\n'
extractx -s 2 *b.krn  | ridx -dCMI | sed 's/^[0-9][0-9]*\.*//g' | grep -v '^!!!' | sort | uniq 

printf '\n\nFunctional A\n'
extractx -s 1 *a.krn  | ridx -dCMI | sed 's/^[0-9][0-9]*\.*//g' | grep -v '^!!!' | sort | uniq 
printf '\n\nFunctional B\n'
extractx -s 1 *b.krn  | ridx -dCMI | sed 's/^[0-9][0-9]*\.*//g' | grep -v '^!!!' | sort | uniq 
