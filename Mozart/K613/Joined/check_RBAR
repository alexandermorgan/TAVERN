ANs=as.numeric(system("grep '^=' *a.krn | sed 's/	.*//g' | sed 's/.*=//g'",intern=T))
BNs=as.numeric(system("grep '^=' *b.krn | sed 's/	.*//g' | sed 's/.*=//g'",intern=T))


ANs=cbind(ANs,c(NA,diff(ANs)))
BNs=cbind(BNs,c(NA,diff(BNs)))

cat('\nA ns\n')
print(ANs[ANs[,2]!=1,])
cat('\nB ns\n')
print(BNs[BNs[,2]!=1,])
