for i in *a.krn 
	do
		CUR=$i
		PHRASE="${CUR%%[ab].*}"
		printf '\n\n\n'
		echo $PHRASE
	A="a.krn"
	B="b.krn"
	extractx -s 1,2 $PHRASE$A > A.krn
	extractx -s 1,2 $PHRASE$B > B.krn
	diff -y --suppress-common-lines A.krn B.krn
	rm A.krn B.krn
	#diff -y --suppress-common-lines $PHRASE$A $PHRASE$B
done	
